	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>

		<from-view-id>/view/login.xhtml</from-view-id>

		<navigation-case>
			<from-outcome>gerente</from-outcome>
			<to-view-id>/view/indexgerente.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>funcionario</from-outcome>
			<to-view-id>/view/indexfuncionario.xhtml</to-view-id>
			<redirect />
		</navigation-case>	

	</navigation-rule>
	
	<navigation-rule>
	
		<from-view-id>/view/checkinpassageiro.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>valido</from-outcome>
			<to-view-id>/view/checkinconfirma.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>
	
	<navigation-rule>
